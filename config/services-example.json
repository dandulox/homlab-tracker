{
  "services": [
    {
      "id": "pihole-main-001",
      "type": "pihole",
      "instanceId": "dns-main",
      "name": "Pi-hole (Main)",
      "url": "http://10.0.20.104/admin",
      "icon": "Shield",
      "group": "Netzwerk",
      "tags": ["dns", "adblock", "primary"],
      "vlan": 20,
      "checks": {
        "ping": {
          "enabled": true,
          "host": "10.0.20.104",
          "intervalSec": 30
        },
        "http": {
          "enabled": true,
          "url": "http://10.0.20.104/admin",
          "intervalSec": 60,
          "expectStatus": 200
        },
        "adapters": {
          "pihole": {
            "enabled": true,
            "baseUrl": "http://10.0.20.104/admin",
            "token": "env:PIHOLE_TOKEN_MAIN",
            "version": "v5"
          }
        }
      },
      "auth": {
        "mode": "header_forward",
        "header_forward": {
          "headerName": "X-Forwarded-User"
        }
      }
    },
    {
      "id": "pihole-backup-002",
      "type": "pihole",
      "instanceId": "dns-backup",
      "name": "Pi-hole (Backup)",
      "url": "http://10.0.40.104/admin",
      "icon": "Shield",
      "group": "Netzwerk",
      "tags": ["dns", "adblock", "backup"],
      "vlan": 40,
      "checks": {
        "ping": {
          "enabled": true,
          "host": "10.0.40.104",
          "intervalSec": 30
        },
        "adapters": {
          "pihole": {
            "enabled": false,
            "baseUrl": "http://10.0.40.104/admin",
            "version": "v5"
          }
        }
      },
      "auth": {
        "mode": "none"
      }
    },
    {
      "id": "portainer-lab-003",
